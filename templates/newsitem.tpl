<tr>
  <td class="table">
    <!-- BEGIN switch SHOW_TOPLINK -->
    <hr>
    <div align="right" class="genmed">
      <a href="#top">Top of this page</a>
    </div>
    <!-- END switch SHOW_TOPLINK -->
   <!-- BEGIN switch PRINTVIEWBUTTON -->
    <div align="left">
        <span class="gensmall">{PRINTVIEWBUTTON}</span>
    </div>
    <!--  END switch PRINTVIEWBUTTON -->
    <!-- BEGIN switch TITLE -->
    <p class="sectiontitle">{TITLE}</p>
    <!-- END switch TITLE -->
    <!-- BEGIN switch LOCATION_DATE -->
    <div>
      <span class="articlelocation">{LOCATION}</span>
      <span class="articledate">{DATE}</span>
    </div>
    <!-- END switch LOCATION_DATE -->
    <!-- BEGIN switch SOURCE -->
    <span class="articlesource">Source:
    <!-- BEGIN switch SOURCE_LINK --><a href="{SOURCE_LINK}" target="_blank"><!-- END switch SOURCE_LINK -->{SOURCE}<!-- BEGIN switch SOURCE_LINK --></a><!-- END switch SOURCE_LINK --></span>
    <!-- END switch SOURCE -->
    <!-- BEGIN switch CONTRIBUTOR -->
    <span class="articlesource">Found by: {CONTRIBUTOR}</span>
    <!-- END switch CONTRIBUTOR -->
    <!-- BEGIN switch SYNOPSIS_IMAGE -->
    <p align="justify" class="articlesynopsis">
      {IMAGE}
      <br>&nbsp;<br>
      {TEXT}
    </p>
    <!-- END switch SYNOPSIS_IMAGE -->
    <!-- BEGIN switch SECTION -->
    {SECTION}
    <!-- END switch SECTION -->
    <!-- BEGIN switch EDITOR -->
    <p class="articledate">Added by {EDITOR}</p>
    <!-- END switch EDITOR -->
    <p class="footer">{COPYRIGHT}</p>
  </td>
</tr>
