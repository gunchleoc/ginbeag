<a name="synopsis"></a>
<div class="contentheader">Synopsis Images</div>
<div class="contentsection">
	{IMAGE}
	<form name="addnewsitemsynposisimageform" action="{ACTIONVARS}" method="post">
		<fieldset>
			<legend class="highlight">Images</legend>
			<label for="{JSID}synopsisimagefilename">Filename:</label>
			<input id="{JSID}synopsisimagefilename" type="text" size="50" maxlength="255" name="filename" />
			<div class="formexplain">A News Item Synopsis can have more than one image. Images are optional.</div>
		</fieldset>
		<input type="submit" name="addnewsitemsynopsisimage" value="Add Image" />
	</form>
</div>
{SYNOPSIS}
